{
  "name": "app-who-metadata",
  "version": "28.0.0",
  "description": "WHO Metadata app",
  "main": "src/index.html",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js && mkdirp build/ && cp src/index.html build/ && cp src/manifest.webapp build/ && mkdirp build/img/icons/ && cp -r src/img/icons/ build/icons/ && cp -r src/i18n/ build/i18n/ && mkdirp build/data/ && cp src/data/whoData.json build/data/ && cp src/runtimeConfig.json build/",
    "postbuild": "mkdirp buildzip/ && pushd build && bestzip ../buildzip/WHOBrowser.zip * && popd",
    "test": "NODE_ENV=test mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test",
    "test-watch": "NODE_ENV=test mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test --watch",
    "lint": "eslint src --quiet",
    "validate": "npm ls",
    "start": "webpack-dev-server --config webpack.dev.config.js"
  },
  "keywords": [
    "DHIS2",
    "App"
  ],
  "author": "Joakim Melseth (cloned from dhis2_app-skeleton https://github.com/dhis2/app-skeleton by Mark Polak)",
  "license": "BSD-3-Clause",
  "dependencies": {
    "axios": "^0.14.0",
    "d2": "30.0.4",
    "d2-ui": "29.0.29",
    "d2-utilizr": "^0.2.15",
    "jquery": "^2.2.1",
    "konva": "^3.0.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.5.0",
    "material-ui": "^0.20.2",
    "moment": "^2.14.1",
    "proptypes": "^1.1.0",
    "react": "~16.8.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "~16.8.3",
    "react-fontawesome": "^1.0.0",
    "react-konva": "^16.8.3",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.7.2",
    "bestzip": "^1.1.3",
    "chai": "^4.1.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "eslint": "^3.19.0",
    "eslint-config-dhis2": "^2.1.0",
    "eslint-plugin-react": "^6.10.3",
    "expose-loader": "^0.7.3",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "isparta": "^4.0.0",
    "jsdom": "^8.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "15.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.13.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "pre-commit": [
    "test",
    "lint",
    "validate"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": ""
